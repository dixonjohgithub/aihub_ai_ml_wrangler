# Database Configuration
DATABASE_URL=postgresql+asyncpg://user:password@localhost:5432/aihub_ml_wrangler
POSTGRES_USER=user
POSTGRES_PASSWORD=password
POSTGRES_DB=aihub_ml_wrangler

# Redis Configuration
REDIS_URL=redis://localhost:6379/0

# Celery Configuration
CELERY_BROKER_URL=redis://localhost:6379/1
CELERY_RESULT_BACKEND=redis://localhost:6379/2

# OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_ORG_ID=optional-org-id

# OpenAI Rate Limiting
OPENAI_MAX_REQUESTS_PER_MINUTE=60
OPENAI_MAX_TOKENS_PER_MINUTE=150000

# OpenAI Caching
OPENAI_CACHE_TTL_HOURS=24
OPENAI_ENABLE_CACHING=true

# OpenAI Cost Tracking
OPENAI_ENABLE_COST_TRACKING=true
OPENAI_COST_ALERT_THRESHOLD=100.00

# Application Settings
APP_ENV=development
DEBUG=true
SECRET_KEY=your-secret-key-here

# CORS Settings
CORS_ORIGINS=http://localhost:3000,http://localhost:5173

# File Upload Settings
MAX_UPLOAD_SIZE_MB=100
ALLOWED_FILE_TYPES=csv,json,xlsx,xls,tsv

# Logging
LOG_LEVEL=INFO
LOG_FILE=app.log